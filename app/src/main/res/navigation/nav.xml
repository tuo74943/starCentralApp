<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/dashboardFragment">
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="io.rosal.starcentral.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_workFormFragment"
            app:destination="@id/workFormFragment" />
    </fragment>
    <fragment
        android:id="@+id/workFormFragment"
        android:name="io.rosal.starcentral.WorkFormFragment"
        android:label="fragment_work_form"
        tools:layout="@layout/fragment_work_form" >
        <action
            android:id="@+id/action_workFormFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment"
            app:popUpTo="@id/dashboardFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>